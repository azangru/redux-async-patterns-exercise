{
  "name": "rutube",
  "version": "0.0.1",
  "description": "Rutube main web site",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf public",
    "prestart": "npm run clean && npm run prepare-config",
    "prepare-config": "gulp config:prepare",
    "start": "webpack --env.dev --watch",
    "start-server": "webpack-dev-server --env.dev --inline --history-api-fallback --content-base public",
    "prebuild": "npm run clean && npm run prepare-config",
    "build": "webpack --env.prod",
    "test": "mocha src/client/tests/.setup.js --reporter spec src/**/*.spec.js",
    "backend-dev": "nodemon src/server/index.js",
    "backend": "NODE_ENV=production forever src/server/index.js"
  },
  "author": "Rutube Development Team",
  "license": "UNLICENSED",
  "dependencies": {
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "draft-js": "^0.8.0",
    "ejs": "^2.5.2",
    "express": "^4.14.0",
    "express-http-proxy": "^0.10.0",
    "forever": "^0.15.2",
    "gulp": "^3.9.1",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^7.1.9",
    "morgan": "^1.7.0",
    "normalizr": "^2.2.1",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "redux": "^3.5.2",
    "redux-saga": "^0.11.1",
    "serialize-javascript": "^1.3.0",
    "urijs": "^1.18.2"
