    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.11.6",
    "babel-root-import": "^4.0.1",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "eslint": "^3.2.2",
    "eslint-plugin-mocha": "^4.3.0",
